@api = http://localhost:3000/empleados

# Obtener todos los empleados
GET {{api}}

# Crear un nuevo empleado
POST {{api}}
Content-Type: application/json

{
    "nombre": "TEST",
    "salario": 1000
}

# Eliminar un empleado (reemplaza :id con el ID del empleado a eliminar)
DELETE {{api}}/:id

# Actualizar un empleado (reemplaza :id con el ID del empleado a actualizar)
PUT {{api}}/:id
Content-Type: application/json

{
    "nombre": "Nuevo Nombre",
    "salario": 1500
}
